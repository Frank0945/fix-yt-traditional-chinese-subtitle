{
  "manifest_version": 3,
  "name": "Youtube 繁體自動翻譯修正",
  "description": "修正Youtube 繁體中文自動翻譯無法正確顯示的錯誤。",
  "version": "1.0.0",
  "icons": {
    "128": "128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/watch*",
        "https://youtube.com/watch*"
      ],
      "js": ["js/vendor.js", "js/content_script.js"]
    }
  ]
}
